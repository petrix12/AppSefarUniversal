{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "barryvdh/laravel-dompdf": "*",
        "consoletvs/charts": "*",
        "guzzlehttp/guzzle": "^7.0.1",
        "hubspot/api-client": "^9.4",
        "hubspot/hubspot-php": "^5.0",
        "intervention/image": "^3.9",
        "irazasyed/telegram-bot-sdk": "^3.9",
        "izniburak/laravel-jotform": "*",
        "jeroennoten/laravel-adminlte": "^3.5",
        "jotform/jotform-api-php": "dev-master",
        "justijndepover/teamleader-api": "^0.7.0",
        "laravel-lang/lang": "~7.0",
        "laravel/framework": "^10.0",
        "laravel/jetstream": "^2.2",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.5",
        "laravelcollective/html": "^6.2",
        "league/flysystem-aws-s3-v3": "^3.0",
        "league/oauth2-client": "^2.7",
        "livewire/livewire": "^2.0",
        "maatwebsite/excel": "^3.1",
        "madeitbelgium/teamleader": "*",
        "proclame/monday-api-laravel": "*",
        "realrashid/sweet-alert": "^7.0",
        "rossjcooper/laravel-hubspot": "^5.0",
        "spatie/laravel-permission": "^5.0",
        "stripe/stripe-php": "*",
        "symfony/console": "*"
    },
    "require-dev": {
        "spatie/laravel-ignition": "^2.0",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helper/sefar.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
